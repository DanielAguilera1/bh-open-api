		"morgan": "1.10.0",
		"nestjs-rate-limit": "1.1.1",
		"reflect-metadata": "0.1.13",
		"rimraf": "3.0.2",
		"rxjs": "7.8.0",
		"typeorm": "0.3.11",
		"xml2js": "0.4.23"
	},
	"devDependencies": {
		"@nestjs/cli": "9.1.8",
